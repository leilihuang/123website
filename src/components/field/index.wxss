.field-box {
  background-color: #e3fffe;
}
.field-box .el-table .warning-row {
  background: #f0f9eb;
}
.field-box .el-tabs__item {
  font-size: 24px;
  height: 43px;
  line-height: 43px;
}
.field-box .el-tabs__item:hover,
.field-box .el-tabs__item.is-active {
  color: #53d3e2;
}
.field-box .el-tabs__header {
  padding-top: 24px;
  margin-bottom: 44px;
}
.field-box .el-tabs__active-bar {
  background: #53d3e2;
}
.field-box .banner-box {
  width: 100%;
  background: url('images/field-banner1920x500.png') center no-repeat;
  background-size: cover;
}
.field-box .con-box {
  margin: 0px auto;
  width: 1200px;
}
.field-box .con-box .g1,
.field-box .con-box .g3,
.field-box .con-box .g4 {
  background: #FFF;
}
.field-box .con-box .mt10 {
  margin-top: 10px;
}
.field-box .con-box .title-bg {
  margin-bottom: 48px;
  background: url('images/field-title-bg.png') no-repeat;
  width: 100%;
  height: 81px;
}
.field-box .con-box .field-floor1-text {
  margin: 0px auto;
  padding-bottom: 48px;
  width: 1032px;
  font-family: SourceHanSansCN-Regular;
  font-size: 24px;
  color: #313131;
  text-indent: 2em;
}
.field-box .con-box .title-text {
  font-family: SourceHanSansCN-Bold;
  font-size: 40px;
  font-weight: normal;
  font-stretch: normal;
  letter-spacing: 0px;
  color: #ffffff;
  text-align: center;
  line-height: 81px;
}
.field-box .con-box .text-wrap {
  width: 1032px;
  margin: 0px auto;
}
.field-box .con-box .text-wrap h4,
.field-box .con-box .text-wrap p {
  font-family: SourceHanSansCN-Bold;
  font-size: 20px;
  letter-spacing: 0px;
  color: #313131;
  text-indent: 2em;
}
.field-box .con-box .text-wrap h4 {
  font-weight: 600;
}
.field-box .con-box .process-wrap {
  margin: 72px auto 0px auto;
  width: 1032px;
}
.field-box .con-box .process-wrap .icon-img {
  display: inline-block;
  width: 52px;
  height: 51px;
  vertical-align: middle;
}
.field-box .con-box .process-wrap .process-title {
  display: flex;
  margin-bottom: 15px;
}
.field-box .con-box .process-wrap .process-title .right,
.field-box .con-box .process-wrap .process-title .error {
  flex: 1;
  font-family: SourceHanSansCN-Bold;
  font-size: 30px;
  color: #313131;
}
.field-box .con-box .process-wrap .process-title .error {
  text-align: right;
}
.field-box .con-box .process-wrap .process-img {
  margin-bottom: 43px;
}
.field-box .con-box .process-wrap .process-bottom-text {
  font-family: SourceHanSansCN-Bold;
  font-size: 20px;
  color: #313131;
  text-indent: 2em;
}
.field-box .con-box .field-con {
  margin: 0px auto;
  width: 1032px;
  text-align: center;
}
.field-box .con-box .field-con .field-oper {
  padding-bottom: 57px;
}
.field-box .con-box .field-con .field-oper h3 {
  margin-bottom: 33px;
  font-size: 36px;
  color: #313131;
  font-family: SourceHanSansCN-Bold;
}
.field-box .con-box .field-con .field-oper p {
  margin-bottom: 34px;
  font-size: 24px;
  color: #313131;
}
.field-box .con-box .field-con .field-oper .oper-button {
  display: inline-block;
  vertical-align: middle;
  width: 284px;
  height: 70px;
  background: url('images/go-detail.png') no-repeat;
  cursor: pointer;
}
.field-box .field-table {
  margin-bottom: 74px;
}
